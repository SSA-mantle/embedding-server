   "source": [
    "import re\n",
    "\n",
    "# 파일 읽기\n",
    "with open(\"input.txt\", \"r\", encoding=\"utf-8\") as f:\n",
    "    text = f.read()\n",
    "\n",
    "# 연속 줄바꿈을 하나로 줄이기\n",
    "text_fixed = re.sub(r'\\n{2,}', '\\n', text)\n",
    "\n",
    "# 결과 다시 파일로 쓰기\n",
    "with open(\"output.txt\", \"w\", encoding=\"utf-8\") as f:\n",
    "    f.write(text_fixed)"
   ]
  }
